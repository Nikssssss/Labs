#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <malloc.h>

void swap(int *c, int *b)
{
	int temp;

	temp = *c;
	*c = *b;
	*b = temp;
}

void heapSort(int *a, int N)
{
	int mid, s, left, right, min;
		
	mid = N / 2 - 1;

	while (N > 1)
	{
		while (mid >= 0)
		{
			s = mid;

			while (s < N)
			{
				min = s;
				left = 2 * s + 1;
				right = 2 * s + 2;

				if (a[min] > a[left] && left < N)
				{
					min = left;
				}
				
				if (a[min] > a[right] && right < N)
				{
					min = right;
				}

				if (min != s) {
					swap(&a[s], &a[min]);
					s = min;
				}
				else break;
			}

			mid--;
		}

		swap(&a[0], &a[N - 1]);

		N--;

		mid = N / 2 - 1;
	}
}

int main()
{
	int *a;
	int N;

	scanf("%d", &N);

	a = (int*)malloc(N * sizeof(int));
		
	for (int i = 0; i < N; i++) 
	{
		scanf("%d", &a[i]);
	}

	heapSort(a, N);

	for (int i = N-1; i >= 0; i--)
	{
		printf("%d ", a[i]);
	}

	free(a);

	return 0;
}

