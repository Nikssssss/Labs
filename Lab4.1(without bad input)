#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void swap(char *a, int c, int b) {
	int temp = a[c];
	a[c] = a[b];
	a[b] = temp;
}

void sort(char *a, int b) {
	int s = 0;
	int len = strlen(a) - 1;
	while (b < len) {
		swap(a, b, len - s);
		b++;
		s++;
	}
}

void permutations(char *a, int N) {
	for (int s = 0; s < N; s++) {
		int len = strlen(a) - 1;
		int i = strlen(a) - 2;
		
		while (i >= 0) {
			
			if (a[i] < a[len]) {
				int min = i + 1;
				for (int j = i + 1; j < strlen(a); j++) {
					if ((a[j] > a[i]) && (a[j] < a[min])) {
						 min = j;
					}
				}
				swap(a, i, min);

				sort(a, i + 1);

				for (int v = 0; v < strlen(a); v++) {
					printf("%c", a[v]);
				}
				printf("\n");

				break;
			}

			
			i--;
			len--;
		}
		
		if (i < 0) {
			printf("");
			break;
		}
	}
}

int main() {
	int N;
	char a[11] = { '\0' };
	
	gets(a);
	scanf("%d", &N);

	permutations(a, N);

	return 0;
}
